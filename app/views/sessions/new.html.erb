<% if flash %>
    <%= flash[:incorrect] %>
<% end %>


<%= form_for @user, url: sessions_path do |f| %>

    
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    
    <%= f.label :password %><br>
    <%= f.password_field :password %><br><br>
    
    <%= f.submit "Sign In" %>


<% end %>